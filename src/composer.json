{
    "require": {
        "sonata-project/admin-bundle": "^3.99",
        "sonata-project/doctrine-orm-admin-bundle": "*",
        "symfony/flex": "*",
        "symfony/security-bundle": "*",
        "symfony/dotenv": "*",
        "doctrine/doctrine-migrations-bundle": "2.*",
        "symfony/yaml": "*",
        "sensio/framework-extra-bundle": "*",
        "sonata-project/user-bundle": "*",
        "symfony/templating": "*",
        "symfony/acl-bundle": "*",
        "sonata-project/block-bundle": ">=3.0 <4.0",
        "sonata-project/cache-bundle": "*",
        "sonata-project/seo-bundle": "*",
        "sonata-project/notification-orm-pack": "*",
        "symfony/mailer": "*"
    },
    "require-dev": {
      "symfony/stopwatch": "*",
      "symfony/twig-bundle": "*",
      "symfony/web-profiler-bundle": "*",
        "doctrine/doctrine-fixtures-bundle": "*",
        "symfony/maker-bundle": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "description": "Just an example of Sonata running in a Docker container."
}
